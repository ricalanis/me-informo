!!!
%html
  %head
    %meta{charset: "utf-8"}
    %meta{content: "IE=edge,chrome=1", http: {equiv: "X-UA-Compatible"}}
    %meta{name: "viewport", content: "width=device-width, initial-scale=1"}
    %title= current_page.data.title || "The Middleman"
    = stylesheet_link_tag "application"

  %body{class: page_classes}

    = yield

    = partial 'footer'

    = partial 'olark'

    = javascript_include_tag "https://code.jquery.com/jquery-2.1.1.min.js"
    = javascript_include_tag "http://d3js.org/d3.v2.js"
    = javascript_include_tag "https://cdn.optimizely.com/js/6258632015.js"
    = javascript_include_tag "application"
    = google_analytics_tag
